{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "PORT": "5000",
      "MONGO_URI": "@mongo_uri",
      "JWT_SECRET": "@jwt_secret",
      "SECRET_KEY": "@secret_key"
    }
  }
  